{
  "expo": {
    "name": "TrackIt",
    "slug": "TrackIt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ic_launcher.png",
    "splash": {
      "image": "./assets/trackit.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFC700"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher.png",
        "backgroundColor": "#FFC700"
      },
      "package": "com.Trackit"
    },
    "web": {
      "favicon": "./assets/ic_launcher.png"
    },
    "androidStatusBar": {
      "translucent": true
    }
  }
}
